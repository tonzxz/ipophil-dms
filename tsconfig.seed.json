{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "lib": [
            "ES2020"
        ],
        "outDir": "./dist",
        "rootDir": "./src",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
        "baseUrl": "."
    },
    "include": [
        "src/lib/dms/seed.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}